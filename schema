CREATE TABLE "relation_Access" (
  "auto_id" INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
  "link" INTEGER,
  "from_id" INTEGER NOT NULL,
  "from_type" INTEGER NOT NULL,
  "to_id" INTEGER NOT NULL,
  "to_type" INTEGER NOT NULL,
  "dir" INTEGER NOT NULL,
  "cost" INTEGER NOT NULL,
  CONSTRAINT "link_fk"
    FOREIGN KEY ("link")
    REFERENCES "relation_Link" ("link")
    DEFERRABLE INITIALLY DEFERRED);
CREATE TABLE "relation_Connect" (
  "auto_id" INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
  "link" INTEGER,
  "dir" INTEGER NOT NULL,
  "to_link" INTEGER,
  "lanes" INTEGER NOT NULL,
  "to_lanes" INTEGER NOT NULL,
  "type" INTEGER NOT NULL,
  "penalty" INTEGER NOT NULL,
  "speed" REAL,
  "capacity" INTEGER NOT NULL,
  "in_high" INTEGER NOT NULL,
  "out_high" INTEGER NOT NULL,
  CONSTRAINT "link_fk"
    FOREIGN KEY ("link")
    REFERENCES "relation_Link" ("link")
    DEFERRABLE INITIALLY DEFERRED,
  CONSTRAINT "to_link_fk"
    FOREIGN KEY ("to_link")
    REFERENCES "relation_Link" ("link")
    DEFERRABLE INITIALLY DEFERRED);
CREATE TABLE "relation_Detector" (
  "detector" INTEGER NOT NULL PRIMARY KEY,
  "link" INTEGER,
  "dir" INTEGER NOT NULL,
  "offset" REAL,
  "length" REAL,
  "lanes" INTEGER NOT NULL,
  "type" INTEGER NOT NULL,
  "use" INTEGER NOT NULL,
  "low" INTEGER NOT NULL,
  "high" INTEGER NOT NULL,
  CONSTRAINT "link_fk"
    FOREIGN KEY ("link")
    REFERENCES "relation_Link" ("link")
    DEFERRABLE INITIALLY DEFERRED);
CREATE TABLE "relation_Driver" (
  "auto_id" INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
  "route" INTEGER NOT NULL,
  "links" INTEGER,
  "type" INTEGER NOT NULL,
  "subtype" INTEGER NOT NULL,
  "link" INTEGER,
  "dir" INTEGER NOT NULL,
  CONSTRAINT "links_fk"
    FOREIGN KEY ("links")
    REFERENCES "relation_Link" ("link")
    DEFERRABLE INITIALLY DEFERRED,
  CONSTRAINT "link_fk"
    FOREIGN KEY ("link")
    REFERENCES "relation_Link" ("link")
    DEFERRABLE INITIALLY DEFERRED);
CREATE TABLE "relation_Event" (
  "auto_id" INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
  "hhold" INTEGER NOT NULL,
  "person" INTEGER NOT NULL,
  "tour" INTEGER NOT NULL,
  "trip" INTEGER,
  "mode" INTEGER NOT NULL,
  "type" INTEGER NOT NULL,
  "link" INTEGER,
  "dir" INTEGER NOT NULL,
  "lane" INTEGER NOT NULL,
  "offset" REAL,
  "route" INTEGER NOT NULL,
  CONSTRAINT "trip_fk"
    FOREIGN KEY ("trip")
    REFERENCES "relation_Trip" ("trip")
    DEFERRABLE INITIALLY DEFERRED,
  CONSTRAINT "link_fk"
    FOREIGN KEY ("link")
    REFERENCES "relation_Link" ("link")
    DEFERRABLE INITIALLY DEFERRED);
CREATE TABLE "relation_Fare" (
  "from_zone" INTEGER,
  "to_zone" INTEGER,
  "from_mode" INTEGER NOT NULL,
  "to_mode" INTEGER NOT NULL,
  "period" INTEGER NOT NULL,
  "type" INTEGER NOT NULL,
  "fare" INTEGER NOT NULL PRIMARY KEY,
  CONSTRAINT "from_zone_fk"
    FOREIGN KEY ("from_zone")
    REFERENCES "relation_Zone" ("zone")
    DEFERRABLE INITIALLY DEFERRED,
  CONSTRAINT "to_zone_fk"
    FOREIGN KEY ("to_zone")
    REFERENCES "relation_Zone" ("zone")
    DEFERRABLE INITIALLY DEFERRED);
CREATE TABLE "relation_Household" (
  "auto_id" INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
  "hhold" INTEGER NOT NULL,
  "location" INTEGER,
  "persons" INTEGER NOT NULL,
  "workers" INTEGER NOT NULL,
  "vehicles" INTEGER,
  "type" INTEGER NOT NULL,
  "partition" INTEGER NOT NULL,
  "person" INTEGER NOT NULL,
  "age" INTEGER NOT NULL,
  "relate" INTEGER NOT NULL,
  "gender" INTEGER NOT NULL,
  "work" INTEGER NOT NULL,
  "drive" INTEGER NOT NULL,
  CONSTRAINT "location_fk"
    FOREIGN KEY ("location")
    REFERENCES "relation_Location" ("location")
    DEFERRABLE INITIALLY DEFERRED,
  CONSTRAINT "vehicles_fk"
    FOREIGN KEY ("vehicles")
    REFERENCES "relation_Vehicle" ("vehicle")
    DEFERRABLE INITIALLY DEFERRED);
