    FOREIGN KEY ("stops")
    REFERENCES "relation_Stop" ("stop")
    DEFERRABLE INITIALLY DEFERRED,
  CONSTRAINT "stop_fk"
    FOREIGN KEY ("stop")
    REFERENCES "relation_Stop" ("stop")
    DEFERRABLE INITIALLY DEFERRED,
  CONSTRAINT "zone_fk"
    FOREIGN KEY ("zone")
    REFERENCES "relation_Zone" ("zone")
    DEFERRABLE INITIALLY DEFERRED);
CREATE TABLE "relation_Link" (
  "link" INTEGER NOT NULL PRIMARY KEY,
  "name" TEXT NOT NULL,
  "node_a" INTEGER,
  "node_b" INTEGER,
  "length" REAL,
  "setback_a" REAL,
  "setback_b" REAL,
  "bearing_a" INTEGER NOT NULL,
  "bearing_b" INTEGER NOT NULL,
  "type" INTEGER NOT NULL,
  "divided" INTEGER NOT NULL,
  "area_type" INTEGER NOT NULL,
  "use" INTEGER NOT NULL,
  "grade" REAL,
  "lanes_ab" INTEGER NOT NULL,
  "speed_ab" REAL,
  "fspd_ab" REAL,
  "cap_ab" INTEGER NOT NULL,
  "lanes_ba" INTEGER NOT NULL,
  "speed_ba" REAL,
  "fspd_ba" REAL,
  "cap_ba" INTEGER NOT NULL,
  "left_ab" INTEGER NOT NULL,
  "right_ab" INTEGER NOT NULL,
  "left_ba" INTEGER NOT NULL,
  "right_ba" INTEGER NOT NULL,
  CONSTRAINT "node_a_fk"
    FOREIGN KEY ("node_a")
    REFERENCES "relation_Node" ("node")
    DEFERRABLE INITIALLY DEFERRED,
  CONSTRAINT "node_b_fk"
    FOREIGN KEY ("node_b")
    REFERENCES "relation_Node" ("node")
    DEFERRABLE INITIALLY DEFERRED);
